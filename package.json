{
  "name": "coc-clangd",
  "version": "0.0.0-development",
  "description": "clangd extension for coc.nvim",
  "author": "Heyward Fann <fannheyward@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "coc.nvim"
  ],
  "engines": {
    "coc": "^0.0.70"
  },
  "scripts": {
    "clean": "rimraf lib",
    "watch": "webpack --watch",
    "build": "webpack",
    "prepare": "npm-run-all clean build"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "coc.nvim": "^0.0.74",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "coc-clangd configuration",
      "properties": {
        "coc-clangd.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable coc-clangd extension"
        }
      }
    },
    "commands": [
      {
        "command": "coc-clangd.Command",
        "title": "coc-clangd command title"
      }
    ]
  }
}
